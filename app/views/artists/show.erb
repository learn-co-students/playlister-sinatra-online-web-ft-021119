<h3>Artist Name:<h3>
<h1><%= @artist.name %></h1>

<h3>Songs:</h3>
<ul>
<% @songs.each do |song| %>
  <h3><li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li></h3>
<% end %>
</ul>

<h3>Genres:</h3>
<ul>
<% @genres.each do |genre| %>
  <h3><li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li></h3>
<% end %>
</ul>
